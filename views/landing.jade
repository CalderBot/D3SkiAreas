				option(value='CA') California
				option(value='CO') Colorado
	script(src='http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js')
	script(src='http://d3js.org/d3.v3.min.js')
	script(src='/landing.js')